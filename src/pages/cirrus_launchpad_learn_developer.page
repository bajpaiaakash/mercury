<apex:page docType="html-5.0" controller="ieCompatibility" standardStylesheets="false" showHeader="false" sidebar="false">
    <head>

        <!-- Meta -->
        <meta charset="UTF-8" />
        <title>Cirrus - Launchpad</title>
        <meta name="keywords" content="Eli Lilly & Company, Lilly, Cirrus, Salesforce.com, SFDC, Force.com" />
        <meta name="description" content="Cirrus Launchpad" />
        <meta name="author" content="Matthew Bull" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />

        <!-- CSS -->
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/reset.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/bootstrap.min.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/font-awesome.min.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/googlefonts.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/idangerous.swiper.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/style.css')}" />

    </head>

    <body>

        <!-- Preloader -->
        <div id="preloader">
            <div id="status"></div>
        </div>

        <!-- Overlay -->
        <div id="overlay"></div>

        <!-- Top header -->
        <div id="top">

            <!-- Sidebar button -->
            <a href="#" id="sidebar-button"></a>

            <!-- Logo -->
            <header id="logo">
                <h1>Cirrus</h1>
                <h2>Simple &amp; Agile</h2>             
            </header>

            <!-- Portfolio close button -->
            <a href="#" id="portfolio-close"></a>

        </div>

        <!-- Main wrapper -->
        <div id="main-wrapper">

            <!-- Content -->
            <div id="content" class="isotope-container">

                <!-- Fluid container -->
                <div class="container-fluid">

                    <!-- Page heading -->
                    <div id="heading" class="row">
                        <div class="filter col-12">

                            <header>
                                <h1>Learn</h1>
                                <h2>Get started with development</h2>
                            </header>

                            <!-- Filters -->
                            <div class="filter-dropdown">
                                <div class="selected"><span class="name">Filter:</span> <span class="val">All</span></div>
                                <ul>
                                    <li data-filter="*">All</li>
                                    <li data-filter=".force">Force.com</li>
                                    <li data-filter=".declarative">Declarative</li>
                                    <li data-filter=".apex">APEX</li>
                                    <li data-filter=".visualforce">Visualforce</li>
                                </ul>
                                <div class="arrow"></div>
                            </div>

                        </div>
                    </div>

                    <!-- Main content -->
                    <div class="isotope isotope-3 row">

                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item force col-4">
                            <a href="#first_app" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/first_app.jpg')}" alt="Build Your First App" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>Build Your First App</h3>
                                </header>
                            </div>
                        </article>

                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item force col-4">
                            <a href="#setup_menu" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/setup_menu.jpg')}" alt="The Force.com Setup Menu" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>The Setup Menu</h3>
                                </header>
                            </div>
                        </article>

                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item declarative col-4">
                            <a href="#declarative" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/declarative.jpg')}" alt="Declarative Overview" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>Declarative Overview</h3>
                                </header>
                            </div>
                        </article>

                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item apex col-4">
                            <a href="#apex" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/apex.jpg')}" alt="APEX Overview" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>APEX Overview</h3>
                                </header>
                            </div>
                        </article>

                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item visualforce col-4">
                            <a href="#visualforce" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/visualforce.jpg')}" alt="Visualforce Overview" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>Visualforce Overview</h3>
                                </header>
                            </div>
                        </article>

                    </div>

                </div>

            </div>

            <!-- Collapsible sidebar -->
            <div id="sidebar">

                <!-- Widget Area -->
                <div id="widgets">

                    <!-- Main menu -->
                    <nav id="mainmenu">
                        <ul>
                            <li><a href="/apex/cirrus_launchpad_vision">Vision</a></li>
                            <li><a href="/apex/cirrus_launchpad_principles">Principles</a></li> 
                            <li><a href="/apex/cirrus_launchpad_success">Success</a></li>
                            <li>
                                <a href="#" class="active">Learn</a>
                                <ul>
                                    <li><a href="/apex/cirrus_launchpad_learn_overview">Overview</a></li>
                                    <li><a href="/apex/cirrus_launchpad_learn_basic">Basic Training</a></li>
                                    <li><a href="/apex/cirrus_launchpad_learn_developer">Developer Training</a></li>
                                    <li><a href="/apex/cirrus_launchpad_learn_play">Start Playing</a></li>
                                </ul>
                            </li>                           
                            <li><a href="/apex/cirrus_launchpad_begin">Next Steps</a></li>
                            <li><a href="/">Back to Force.com</a></li>
                        </ul>
                    </nav>

                </div>

                <!-- Copyright -->
                <footer>
                    <p class="copyright">Eli Lilly &amp; Company</p>
                </footer>

            </div>

            <!-- Portfolio Content -->
            <div id="first_app" class="portfolio-full portfolio-full-slidedown">

                <!-- Portfolio description -->
                <div class="portfolio-description">
                    <h3>Build Your First App</h3>
                    <p>A short video demonstrating a Force.com app being built in under six minutes!</p> 
                    <div class="video"><iframe class="vidyard_iframe" src="https://play.vidyard.com/q4F1G-qrBDO92a8V-thpJQ.html?v=3.1" width="640" height="360" scrolling="no" frameborder="0" allowtransparency="true"></iframe></div>
                    <p>
                    <div class="icon-text">
                        <i class="fa fa-comments"></i>
                        <p><a href="/_ui/core/chatter/groups/GroupProfilePage?g=0F9b0000000HZZz">Ask a question on Chatter (Hashtag: #Force.com)</a></p>
                    </div>
                    </p>
                </div>

            </div>

            <!-- Portfolio Content -->
            <div id="setup_menu" class="portfolio-full portfolio-full-slidedown">

                <!-- Portfolio description -->
                <div class="portfolio-description">
                    <h3>The Force.com Setup Menu</h3>
                    <p>The Force.com admin setup menu can be overwhelming at first; however, just like a new city gets smaller as you learn your way around, so does the admin setup. There are several tools available within the admin setup to help you find answers to questions you may have or problems that you face. In addition, there are tools to help you work faster within the setup with minimal clicking and redirecting. </p> 
                    <div class="video"><object width="640" height="360"><param name="movie" value="https://www.youtube.com/v/kWqm8OWikOU?version=3&amp;hl=en_GB&amp;rel=0"></param><param name="allowFullScreen" value="true"></param><param name="allowscriptaccess" value="always"></param><embed src="https://www.youtube.com/v/kWqm8OWikOU?version=3&amp;hl=en_GB&amp;rel=0" type="application/x-shockwave-flash" width="640" height="360" allowscriptaccess="always" allowfullscreen="true"></embed></object></div>
                    <p>
                    <div class="icon-text">
                        <i class="fa fa-comments"></i>
                        <p><a href="/_ui/core/chatter/groups/GroupProfilePage?g=0F9b0000000HZZz">Ask a question on Chatter (Hashtag: #Force.com)</a></p>
                    </div>
                    </p>
                </div>

            </div>
            
            <!-- Portfolio Content -->
            <div id="declarative" class="portfolio-full portfolio-full-slidedown">

                <!-- Portfolio description -->
                <div class="portfolio-description">
                    <h3>Declarative Overview</h3>
                    <p>Force.com is the worlds leading cloud platform that lets you build apps rapidly using configuration-driven development and powerful programmatic logic. With Force.com, you can design mobile, social, and real-time apps in the cloud five times faster than with traditional software development approaches.</p> 
                    <div class="video"><object width="640" height="360"><param name="movie" value="https://www.youtube.com/v/ee1ncea0eeE?version=3&amp;hl=en_GB&amp;rel=0"></param><param name="allowFullScreen" value="true"></param><param name="allowscriptaccess" value="always"></param><embed src="https://www.youtube.com/v/ee1ncea0eeE?version=3&amp;hl=en_GB&amp;rel=0" type="application/x-shockwave-flash" width="640" height="360" allowscriptaccess="always" allowfullscreen="true"></embed></object></div>
                    <p>
                    <div class="icon-text">
                        <i class="fa fa-comments"></i>
                        <p><a href="/_ui/core/chatter/groups/GroupProfilePage?g=0F9b0000000HZZz">Ask a question on Chatter (Hashtag: #Force.com)</a></p>
                    </div>
                    </p>
                </div>

            </div>

            <!-- Portfolio Content -->
            <div id="apex" class="portfolio-full portfolio-full-slidedown">

                <!-- Portfolio description -->
                <div class="portfolio-description">
                    <h3>APEX Overview</h3>
                    <p>Whether you are an experienced programmer or a salesforce.com administrator, Apex can help you get more out of the Force.com platform. Apex is the programming language of Force.com; it allows you to go beyond the point-and-click framework to add specialized and complex logic to your applications.</p> 
                    <div class="video"><object width="640" height="360"><param name="movie" value="https://www.youtube.com/v/J4yX17_yxbI?version=3&amp;hl=en_GB&amp;rel=0"></param><param name="allowFullScreen" value="true"></param><param name="allowscriptaccess" value="always"></param><embed src="https://www.youtube.com/v/J4yX17_yxbI?version=3&amp;hl=en_GB&amp;rel=0" type="application/x-shockwave-flash" width="640" height="360" allowscriptaccess="always" allowfullscreen="true"></embed></object></div>
                    <p>
                    <div class="icon-text">
                        <i class="fa fa-comments"></i>
                        <p><a href="/_ui/core/chatter/groups/GroupProfilePage?g=0F9b0000000HZZz">Ask a question on Chatter (Hashtag: #APEX)</a></p>
                    </div>
                    </p>
                </div>

            </div>
            
            <!-- Portfolio Content -->
            <div id="visualforce" class="portfolio-full portfolio-full-slidedown">

                <!-- Portfolio description -->
                <div class="portfolio-description">
                    <h3>Visualforce Overview</h3>
                    <p>Out of the box, the Force.com Platform can automatically generate user interfaces, but in some cases you might want to build a more custom UI. Join us to learn about Visualforce, the component-based UI framework that lets you build attractive, dynamic, reusable user interfaces.</p> 
                    <div class="video"><object width="640" height="360"><param name="movie" value="https://www.youtube.com/v/wo74xdvtvUE?hl=en_GB&amp;version=3&amp;rel=0"></param><param name="allowFullScreen" value="true"></param><param name="allowscriptaccess" value="always"></param><embed src="https://www.youtube.com/v/wo74xdvtvUE?hl=en_GB&amp;version=3&amp;rel=0" type="application/x-shockwave-flash" width="640" height="360" allowscriptaccess="always" allowfullscreen="true"></embed></object></div>
                    <p>
                    <div class="icon-text">
                        <i class="fa fa-comments"></i>
                        <p><a href="/_ui/core/chatter/groups/GroupProfilePage?g=0F9b0000000HZZz">Ask a question on Chatter (Hashtag: #Visualforce)</a></p>
                    </div>
                    </p>
                </div>

            </div>

        </div>

        <!-- JavaScript -->
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/jquery.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/bootstrap.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/swiper/idangerous.swiper.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/masonry/masonry.pkgd.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/isotope/jquery.isotope.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/custom.js')}" />

    </body>
</apex:page>
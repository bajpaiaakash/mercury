<apex:page docType="html-5.0" controller="ieCompatibility" standardStylesheets="false" showHeader="false" sidebar="false">
    <head>

        <!-- Meta -->
        <meta charset="UTF-8" />
        <title>Cirrus - Launchpad</title>
        <meta name="keywords" content="Eli Lilly & Company, Lilly, Cirrus, Salesforce.com, SFDC, Force.com" />
        <meta name="description" content="Cirrus Launchpad" />
        <meta name="author" content="Matthew Bull" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />

        <!-- CSS -->
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/reset.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/bootstrap.min.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/font-awesome.min.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/googlefonts.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/idangerous.swiper.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.cirrus_launchpad, 'css/style.css')}" />

    </head>

    <body>

        <!-- Preloader -->
        <div id="preloader">
            <div id="status"></div>
        </div>

        <!-- Overlay -->
        <div id="overlay"></div>

        <!-- Top header -->
        <div id="top">

            <!-- Sidebar button -->
            <a href="#" id="sidebar-button"></a>

            <!-- Logo -->
            <header id="logo">
                <h1>Cirrus</h1>
                <h2>Simple &amp; Agile</h2> 
            </header>

            <!-- Portfolio close button -->
            <a href="#" id="portfolio-close"></a>

        </div>

        <!-- Main wrapper -->
        <div id="main-wrapper">

            <!-- Content -->
            <div id="content" class="isotope-container">

                <!-- Fluid container -->
                <div class="container-fluid">

                    <!-- Page heading -->
                    <div id="heading" class="row">
                        <div class="filter col-12">

                            <header>
                                <h1>Success</h1>
                                <h2>Be inspired</h2>
                            </header>

                            <!-- Filters -->
                            <div class="filter-dropdown">
                                <div class="selected"><span class="name">Filter:</span> <span class="val">All</span></div>
                                <ul>
                                    <li data-filter="*">All</li>
                                    <li data-filter=".sales">Sales</li>
                                    <li data-filter=".service">Service</li>
                                    <li data-filter=".marketing">Marketing</li>
                                    <li data-filter=".force">Force.com</li>
                                    <li data-filter=".heroku">Heroku</li>
                                    <li data-filter=".appexchange">AppExchange</li>
                                </ul>
                                <div class="arrow"></div>
                            </div>

                        </div>
                    </div>
                    
                    <div class="alert alert-info fade in">
                        <i class="fa fa-info-circle"></i>
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                        <p>More Information Coming Soon!</p>
                    </div>
                    
                    <!-- Main content -->
                    <div class="isotope isotope-2 row">

                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item sales col-6">
                            <a href="#" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/veeva_crm.jpg')}" alt="Veeva CRM" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>VEEVA CRM</h3>
                                </header>
                            </div>
                        </article>

                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item service appexchange col-6">
                            <a href="#" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/gcc.jpg')}" alt="Global Customer Connect" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>Global Customer Connect</h3>
                                </header>
                            </div>
                        </article>

                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item sales col-6">
                            <a href="#" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/trialforce.jpg')}" alt="Trialforce" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>Trialforce</h3>
                                </header>
                            </div>
                        </article>

                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item force service marketing heroku appexchange col-6">
                            <a href="#" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/mercury.jpg')}" alt="Mercury" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>Mercury</h3>
                                </header>
                            </div>
                        </article>

                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item force col-6">
                            <a href="#" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/matchmaker.jpg')}" alt="Matchmaker" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>Matchmaker</h3>
                                </header>
                            </div>
                        </article>

                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item force col-6">
                            <a href="#" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/cymbalta_pe.jpg')}" alt="Cymbalta PE" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>Cymbalta PE</h3>
                                </header>
                            </div>
                        </article>
                        
                        <!-- Portfolio item -->
                        <article class="portfolio isotope-item sales col-6">
                            <a href="#" class="image">
                                <apex:image url="{!URLFOR($Resource.cirrus_launchpad, '/img/portfolio-images/elanco_crm.jpg')}" alt="Elanco CRM" />
                            </a>
                            <div class="inner">
                                <header>
                                    <h3>Elanco CRM</h3>
                                </header>
                            </div>
                        </article>

                    </div>

                </div>

            </div>

            <!-- Collapsible sidebar -->
            <div id="sidebar">

                <!-- Widget Area -->
                <div id="widgets">

                    <!-- Main menu -->
                    <nav id="mainmenu">
                        <ul>
                            <li><a href="/apex/cirrus_launchpad_vision">Vision</a></li>
                            <li><a href="/apex/cirrus_launchpad_principles">Principles</a></li> 
                            <li><a href="/apex/cirrus_launchpad_success" class="active">Success</a></li>
                            <li>
                                <a href="#">Learn</a>
                                <ul>
                                    <li><a href="/apex/cirrus_launchpad_learn_overview">Overview</a></li>
                                    <li><a href="/apex/cirrus_launchpad_learn_basic">Basic Training</a></li>
                                    <li><a href="/apex/cirrus_launchpad_learn_developer">Developer Training</a></li>
                                    <li><a href="/apex/cirrus_launchpad_learn_play">Start Playing</a></li>
                                </ul>
                            </li>                           
                            <li><a href="/apex/cirrus_launchpad_begin">Next Steps</a></li>
                            <li><a href="/">Back to Force.com</a></li>
                        </ul>
                    </nav>

                </div>

                <!-- Copyright -->
                <footer>
                    <p class="copyright">Eli Lilly &amp; Company</p>
                </footer>

            </div>

            <!-- Portfolio Content -->
            <div id="agile" class="portfolio-full portfolio-full-slidedown">

                <!-- Portfolio description -->
                <div class="portfolio-description">
                    <h3>Example Title</h3>
                    <p>Say Something Clever!</p>
                    <p>
                    <div class="icon-text">
                        <i class="fa fa-comments"></i>
                        <p><a href="/_ui/core/chatter/groups/GroupProfilePage?g=0F9b0000000HZZz">Ask a question on Chatter (Hashtag: #Agile)</a></p>
                    </div>
                    </p>
                </div>

            </div>

        </div>

        <!-- JavaScript -->
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/jquery.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/bootstrap.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/swiper/idangerous.swiper.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/masonry/masonry.pkgd.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/isotope/jquery.isotope.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.cirrus_launchpad, 'js/custom.js')}" />

    </body>
</apex:page>
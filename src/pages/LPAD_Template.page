<apex:page id="LPAD_Template" showheader="false" sidebar="false" standardStylesheets="false">
	
	<head>
        <meta charset="utf-8" />
        <title>Launchpad</title>
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="apple-mobile-web-app-capable" content="yes" />
        <meta name="mobile-web-app-capable" content="yes" />
        <meta name="description" content="" />
        <meta name="viewport" content="width=device-width" />
        <link rel="shortcut icon" href="/favicon.ico" />
        
		<apex:stylesheet value="{!URLFOR($Resource.LPAD_Assets, 'styles/vendor.css')}" />
		<apex:stylesheet value="{!URLFOR($Resource.LPAD_Assets, 'styles/main.css')}" />
	</head>
	
	<body>
		<script src="{!URLFOR($Resource.LPAD_Assets, 'scripts/vendor.js')}" />
		<script src="{!URLFOR($Resource.LPAD_Assets, 'scripts/plugins.js')}" />
		<script src="{!URLFOR($Resource.LPAD_Assets, 'scripts/main.js')}" />
		
		<apex:insert name="header">
			<c:LPAD_Header />
		</apex:insert>
		  
		<apex:insert name="body">
		</apex:insert>
		
		<apex:insert name="footer">
			<c:LPAD_Footer />
		</apex:insert>
	</body>
	
</apex:page>
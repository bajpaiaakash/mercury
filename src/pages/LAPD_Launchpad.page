<apex:page docType="html-5.0" standardStylesheets="false" showHeader="false" sidebar="false">

<apex:composition template="LAPD_Template">

    <apex:define name="main">

        <section id="page-title">

            <div class="container clearfix">
                <h1>Launchpad</h1>
            </div>
        
        </section>

        <section id="content">

            <div class="content-wrap">

                <div class="container clearfix">

                    <div class="col_full clearfix">
                        <h4>Launchpad Release Schedule</h4>
                    </div>

                    <div class="col_full clearfix">

                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">Launchpad v2.1 (JUL-2015)</h3>
                            </div>
                            <div class="panel-body">
                                Integration with Contentful by Cirrus (API-CMS).
                            </div>
                        </div>
                        <div class="panel panel-success">
                            <div class="panel-heading">
                                <h3 class="panel-title">Launchpad v2.0.3 (Current)</h3>
                            </div>
                            <div class="panel-body">
                                <strong>Minor Update</strong> - Updated ForceFest II: Tour de Force Information. 
                            </div>
                        </div>
                        <div class="panel panel-danger">
                            <div class="panel-heading">
                                <h3 class="panel-title">Launchpad v2.0.2 (MAY-2015)</h3>
                            </div>
                            <div class="panel-body">
                                <strong>Minor Update</strong> - Updated Home Page and General Bug Fixes. 
                            </div>
                        </div>
                        <div class="panel panel-danger">
                            <div class="panel-heading">
                                <h3 class="panel-title">Launchpad v2.0.1 (MAY-2015)</h3>
                            </div>
                            <div class="panel-body">
                                <strong>Minor Update</strong> - General Bug Fixes. 
                            </div>
                        </div>
                        <div class="panel panel-danger">
                            <div class="panel-heading">
                                <h3 class="panel-title">Launchpad v2.0 (MAY-2015)</h3>
                            </div>
                            <div class="panel-body">
                                <strong>Major Update</strong> - An all new design, including updated content for Cirrus, Force.com, Contentful, Architecture and more.
                            </div>
                        </div>
                        <div class="panel panel-danger">
                            <div class="panel-heading">
                                <h3 class="panel-title">Launchpad v1.0 (2014)</h3>
                            </div>
                            <div class="panel-body">
                                Initial Release, including Force.com Process, Success Stories and LillyFit.
                            </div>
                        </div>

                    </div>

                    <div class="line"></div>

                    <div class="col_full">
                        <h4>Launchpad Features</h4>
                    </div>

                    <div class="col_one_third">
                        <div class="feature-box fbox-small fbox-plain" data-animate="fadeIn">
                            <div class="fbox-icon">
                                <a href="#"><i class="icon-line-monitor"></i></a>
                            </div>
                            <h3>Responsive Layout</h3>
                            <p>Fully responsive layout that can be adapted to any screen size.</p>
                        </div>
                    </div>

                    <div class="col_one_third">
                        <div class="feature-box fbox-small fbox-plain" data-animate="fadeIn" data-delay="200">
                            <div class="fbox-icon">
                                <a href="#"><i class="icon-line-eye"></i></a>
                            </div>
                            <h3>Retina Ready Graphics</h3>
                            <p>Graphics that look beautiful and ultra-sharp on Retina Displays (UHD+).</p>
                        </div>
                    </div>

                    <div class="col_one_third col_last">
                        <div class="feature-box fbox-small fbox-plain" data-animate="fadeIn" data-delay="600">
                            <div class="fbox-icon">
                                <a href="#"><i class="icon-line-play"></i></a>
                            </div>
                            <h3>Embeded Media</h3>
                            <p>Native HTML5 Videos, as well as Kaltura and YouTube integration.</p>
                        </div>
                    </div>

                    <div class="clear"></div>

                    <div class="col_one_third">
                        <div class="feature-box fbox-small fbox-plain" data-animate="fadeIn" data-delay="800">
                            <div class="fbox-icon">
                                <a href="#"><i class="icon-params"></i></a>
                            </div>
                            <h3>HTML5 + Visualforce</h3>
                            <p>Easily customizable, following industry standards.</p>
                        </div>
                    </div>

                    <div class="col_one_third">
                        <div class="feature-box fbox-small fbox-plain" data-animate="fadeIn" data-delay="1000">
                            <div class="fbox-icon">
                                <a href="#"><i class="icon-line-circle-check"></i></a>
                            </div>
                            <h3>Pattern Library</h3>
                            <p>A full<apex:outputLink value="{! $Page.LAPD_PatternLibrary}"> Pattern Library</apex:outputLink> of reusable components to enable rapid development.</p>
                        </div>
                    </div>

                    <div class="col_one_third bottommargin-sm col_last">
                        <div class="feature-box fbox-small fbox-plain" data-animate="fadeIn" data-delay="1000">
                            <div class="fbox-icon">
                                <a href="#"><i class="icon-note"></i></a>
                            </div>
                            <h3>SCM Documentation</h3>
                            <p>Fully documented application stored in BitBucket (SCM).</p>
                        </div>
                    </div>

                    <div class="clear"></div>

                </div>

                <c:LAPD_BannerChatter />

            </div>

        </section>

    </apex:define>
    
</apex:composition>

</apex:page>
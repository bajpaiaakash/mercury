        <errorMessage><!-- ToV has already been reported for this meeting. Customer Meeting Services need to confirm and implement this change.  All related ToV values against the meeting should be updated to align with the new meeting currency. --></errorMessage>
        <name>MERC_Confirm_ToV_Reporting_Change</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Meeting must have occurred in order to Complete the Meeting. --></errorMessage>
        <name>MERC_Event_Occurred_before_Complete</name>
    </validationRules>

        <label>Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>AggregateOperation_GLBL__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Rollup operation.</inlineHelpText>

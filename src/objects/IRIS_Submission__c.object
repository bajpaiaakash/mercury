        <fullName>Intended_Audience_Local_Iriss</fullName>
        <active>true</active>
        <description>Validate when Intended Audience is Regional then user should select at least one Country.</description>
        <errorConditionFormula>AND(ISPICKVAL(Intended_Audience__c, &apos;Local&apos;), OR( Country_Text__c = $Label.Iriss_Pre_Text_For_Country ,  ISNULL(Country_Text__c) ,  ISBLANK(Country_Text__c) ))</errorConditionFormula>
        <errorMessage>Please select a Country when Intended Audience is Local.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Intended_Audience_Regional_Iriss</fullName>
        <active>true</active>
        <description>Validate when Intended Audience is Regional then user should select at least one  Region.</description>
        <errorConditionFormula>AND(ISPICKVAL(Intended_Audience__c, &apos;Regional&apos;),OR( Region_Text__c =  $Label.Iriss_Pre_Text_For_Region, ISNULL(Region_Text__c) , ISBLANK(Region_Text__c) ))</errorConditionFormula>
        <errorMessage>Please select a region when Intended Audience is Regional.</errorMessage>
    </validationRules>

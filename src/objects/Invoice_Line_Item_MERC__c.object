    <fields>
        <fullName>Cost_Element_MERC__c</fullName>
        <description>Selection of cost elements to select for Invoice items 
Created by John A Adams 10/15/2013
NOTE: needs final list from Lilly</description>
        <externalId>false</externalId>
        <label>Cost Element</label>
        <picklist>
            <picklistValues>
                <fullName>Food &amp; Beverage</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Audio Visual</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Transportation</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Accommodation</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Description_MERC__c</fullName>
        <description>Line Item Description
Created by John A Adams 10/4/2013</description>
        <externalId>false</externalId>
        <label>Description</label>
        <length>80</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Invoice_MERC__c</fullName>
        <description>Master-Detail relationship to Invoice object. Created 9/28/2013 by KLorenti</description>
        <externalId>false</externalId>
        <label>Invoice</label>
        <referenceTo>Invoice_MERC__c</referenceTo>
        <relationshipLabel>Invoice Line Items</relationshipLabel>
        <relationshipName>Invoice_Line_Items</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Tax_Amount_MERC__c</fullName>
        <description>Tax Amount (currency, calc based on value &amp; tax rate) 
Created by John A Adams 10/4/2013</description>
        <externalId>false</externalId>
        <formula>Value_MERC__c  *  (Tax_Rate_MERC__c / 100)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Tax Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Tax_Rate_MERC__c</fullName>
        <description>Tax Rate Percentage
Created by John A Adams 10/4/2013</description>
        <externalId>false</externalId>
        <label>Tax Rate %</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Value_MERC__c</fullName>
        <description>VaLue of Line item</description>
        <externalId>false</externalId>
        <label>Value</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>
    <label>Invoice Line Item</label>
    <nameField>
        <displayFormat>IL-{000000}</displayFormat>
        <label>Invoice Line Item Number</label>
        <trackHistory>false</trackHistory>

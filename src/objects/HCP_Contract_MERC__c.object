    <fields>
        <fullName>Contract_Follow_Up_Date_MERC__c</fullName>
        <description>Date on which user is to follow up on HCP signature.  For use in SLA Reporting : MERC : Created on 25/02/2105 by Mike Cashman, Mavens Consulting</description>
        <externalId>false</externalId>
        <inlineHelpText>Defaults to +7 Days when the Contract Status is set to &quot;Sent&quot;.  This can be updated manually by CMS Cork and is used to report on which Contracts need chasing up should no signatures be received.</inlineHelpText>
        <label>Contract Follow Up Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>

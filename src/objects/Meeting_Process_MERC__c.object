    <fields>
        <fullName>Process_End_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Process_Status_MERC__c != &apos;In Progress&apos;,
    Max_Completed_Date_MERC__c,
    null
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Process End Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Process_Start_Date_MERC__c</fullName>
        <description>Calculate the date the process started : MERC : Created on 17/06/2015 by Kyle Thornton, Mavens Consulting</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Min_Completed_Date_MERC__c)|| Min_Completed_Date_MERC__c&gt;DATEVALUE(CreatedDate),
   DATEVALUE(CreatedDate),
   Min_Completed_Date_MERC__c
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Process Start Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Process_Status_MERC__c</fullName>
        <description>Calculates the status of the process based on the counts of children tasks : MERC : Created on 17/06/2015 by Kyle Thornton, Mavens Consulting</description>
        <externalId>false</externalId>
        <formula>IF( Number_of_Unresponsive_Tasks_MERC__c &gt; 0, 
   &apos;Unresponsive&apos;,
    IF( Number_of_Cancelled_Tasks_MERC__c &gt; 0,
        &apos;Cancelled&apos;,
        IF( Number_of_Tasks_MERC__c = Number_of_Completed_Tasks_MERC__c,
            &apos;Completed&apos;,
            &apos;In Progress&apos;
        )
    )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Process Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>

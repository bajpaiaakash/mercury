        <fullName>Process_End_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Process_Status_MERC__c != &apos;In Progress&apos;,
    Max_Completed_Date_MERC__c,
    null
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>

<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Indicates an object that needs to be set for &quot;clear down&quot; when a meeting is closed. &quot;Clear down&quot; involves removal of any personal information and will be handled by workflow rules.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Clear_Down_Flag_Name_MERC__c</fullName>
        <description>The api name of the boolean field which triggers the workflows to clear the information from the record.</description>
        <externalId>false</externalId>
        <label>Clear Down Flag Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Meeting_Participant_is_Child_Object_MERC__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Indicates the Meeting Participant object is a child of the configured object. i.e. there is a lookup from the meeting participant to the object.</description>
        <externalId>false</externalId>
        <inlineHelpText>Indicates the Meeting Participant object is a child of the configured object. i.e. there is a lookup from the meeting participant to the object.</inlineHelpText>
        <label>Meeting Participant is Child Object</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Participant_Relationship_Field_MERC__c</fullName>
        <description>The api name of the lookup field which relates to the meeting participant object.</description>
        <externalId>false</externalId>
        <inlineHelpText>The api name of the lookup field which relates to the meeting participant object.</inlineHelpText>
        <label>Participant Relationship Field</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Non-HCP Clear-Down Object</label>
</CustomObject>

    <fields>
        <fullName>Library_Name_MERC__c</fullName>
        <description>Populates the name of a Library Field.
Created on 4/3/2014 by John Adams</description>
        <externalId>false</externalId>
        <formula>IF(CONTAINS(RecordType.Name,&quot;Invoice_MERC&quot;),Invoice_MERC__r.Payee_Country_MERC__c     &amp; &quot; - Private&quot;,
 IF(CONTAINS(RecordType.Name,&quot;HCP_Contract_MERC&quot;), HCP_Contract_MERC__r.Country_of_Residence_MERC__c &amp; &quot; - Private&quot;,
   IF(CONTAINS(RecordType.Name,&quot;Budget_MERC&quot;), TEXT(Meeting_MERC__r.Event_Country_MERC__c) &amp; &quot; - Private&quot;,
  
    IF(CONTAINS(RecordType.Name,&quot;Meeting_Participant_MERC&quot;), Meeting_Participant_MERC__r.Country_of_Residence_MERC__c &amp; &quot; - Private&quot;,

      IF(CONTAINS(RecordType.Name,&quot;Meeting_MERC&quot;), Meeting_Country_MERC__c  &amp; &quot; - Public&quot;,
&quot;Mercury&quot;)
      )

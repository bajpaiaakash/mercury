    <validationRules>
        <fullName>Mercury_User_or_External_User_MERC</fullName>
        <active>true</active>
        <errorConditionFormula>Account_MERC__c != &apos;&apos; &amp;&amp;  User_MERC__c != &apos;&apos;</errorConditionFormula>
        <errorMessage>A Team Member must be associated with either an External Account or a Mercury User.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Single_Primary_Meeting_Owner_MERC</fullName>
        <active>true</active>
        <description>Should only have one Primary Meeting Owner</description>
        <errorConditionFormula>AND (
ISPICKVAL( Role_MERC__c,&quot;Primary Meeting Owner&quot;),
OR(ISNULL(Meeting_MERC__r.Prmry_Mtng_Ownr_Nm_MERC__c), 
Meeting_MERC__r.Prmry_Mtng_Ownr_Nm_MERC__c  &lt;&gt; &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Role_MERC__c</errorDisplayField>
        <errorMessage>Only one team member can be the Primary Meeting Owner. Please chose a different role for this or existing team member.</errorMessage>
    </validationRules>

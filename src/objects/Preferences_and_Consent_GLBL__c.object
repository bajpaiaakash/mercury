    <fields>
        <fullName>Active_MERC__c</fullName>
        <description>Determines whether a Preference or Consent record is active or not. Created 06/199/2014 by KLorenti, Mavens Consulting</description>
        <externalId>false</externalId>
        <formula>(ISBLANK(Efctv_End_Dt_GLBL__c)||
Efctv_End_Dt_GLBL__c &gt;= Today())

|| 

(ISBLANK(Efctv_End_Dt_Cap_GLBL__c) ||
Efctv_End_Dt_Cap_GLBL__c &gt;= Today())</formula>
        <label>Active</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>

    <fields>
        <fullName>Combined_ID_GLBL__c</fullName>
        <description>Concatenation of Account ID and Preference type. Used to enforce uniqueness with validation rule. Created 04/07/2014 by KLorenti, Mavens Consulting</description>
        <externalId>false</externalId>
        <formula>IF 
(RecordType.Name = &quot;Preferences&quot; &amp;&amp; TEXT(Preference_Type_MERC__c) &lt;&gt; &quot;Meals Allergy Preferences&quot;, Account_GLBL__r.Id + &quot;-&quot; + TEXT(Preference_Type_MERC__c),

 IF 
 (RecordType.Name = &quot;Preferences&quot; &amp;&amp; TEXT(Preference_Type_MERC__c) = &quot;Meals Allergy Preferences&quot;, Account_GLBL__r.Id + &quot;-&quot; + TEXT(Preference_Type_MERC__c) + &quot;-&quot; + TEXT( Preference_Sub_Type_MERC__c),
  
   IF
   (RecordType.Name = &quot;Consent&quot; &amp;&amp; Active_MERC__c &amp;&amp; !ISBLANK(Cmnctn_Chnl_Cd_Cap_GLBL__c), Account_GLBL__r.Id + &quot;-&quot; +   Cmnctn_Chnl_Cd_Cap_GLBL__c , 

    

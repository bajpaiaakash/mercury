    <validationRules>
        <fullName>Check_if_any_ToV_has_been_reported</fullName>
        <active>true</active>
        <description>Checks to see if any Meeting Participants have had ToV reported. Created 01/08/2014 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>ISPICKVAL( Meeting_MERC__r.Participant_ToV_Final_MERC__c,&quot;Yes&quot;) &amp;&amp;
(ISCHANGED ( Daily_Registration_Amount_MERC__c ) ||
ISCHANGED ( Daily_Ground_Trans_Amount_MERC__c) ||
ISCHANGED ( Daily_Food_Beverage_Amount_MERC__c )) &amp;&amp;
! Confirm_ToV_Reporting_Change_MERC__c</errorConditionFormula>
        <errorDisplayField>Confirm_ToV_Reporting_Change_MERC__c</errorDisplayField>
        <errorMessage>ToV has already been reported for participants of this Meeting. Customer Meeting Services needs to confirm this change before it can be made.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Blank_If_Not_Reported_MERC</fullName>
        <active>true</active>
        <description>Users cannot enter a 0.00 if ToV has not been reported.</description>
        <errorConditionFormula>(Daily_Food_Beverage_Amount_MERC__c = 0.00 ||
Daily_Ground_Trans_Amount_MERC__c = 0.00 ||
Daily_Registration_Amount_MERC__c = 0.00) &amp;&amp;  (ISPICKVAL(Meeting_MERC__r.Participant_ToV_Final_MERC__c,&quot;No&quot;) || ISPICKVAL(Meeting_MERC__r.Participant_ToV_Final_MERC__c,&quot;&quot;))</errorConditionFormula>
        <errorMessage>You may not enter 0.00 unless ToV has already been reported for this Meeting</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Zero_If_Reported_MERC</fullName>
        <active>true</active>
        <description>Users cannot enter a blank if ToV has been reported.</description>
        <errorConditionFormula>((ISCHANGED(Daily_Food_Beverage_Amount_MERC__c) &amp;&amp; 
ISBLANK(Daily_Food_Beverage_Amount_MERC__c)) || 

(ISCHANGED(Daily_Ground_Trans_Amount_MERC__c) &amp;&amp; 
ISBLANK(Daily_Ground_Trans_Amount_MERC__c)) || 

(ISCHANGED(Daily_Registration_Amount_MERC__c) &amp;&amp; 
ISBLANK(Daily_Registration_Amount_MERC__c)) )

&amp;&amp; (ISPICKVAL(Meeting_MERC__r.Participant_ToV_Final_MERC__c,&quot;Yes&quot;))</errorConditionFormula>
        <errorMessage>You may not enter a blank value if ToV has already been reported for this Meeting</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Meeting_Day_Range</fullName>
        <active>true</active>
        <description>Ensure that a Meeting Date is within 3 days +/- of the Meeting Start or End Date.  This is to accommodate shoulder dates but also limit the dates that can be chosen for Meeting Days.  Created by J. Adams 2/3/2014</description>
        <errorConditionFormula>(Date_MERC__c &gt; (Meeting_MERC__r.End_Date_of_Event_MERC__c + 3))  ||
(Date_MERC__c &lt; (Meeting_MERC__r.Date_of_Event_MERC__c  - 3))</errorConditionFormula>
        <errorDisplayField>Date_MERC__c</errorDisplayField>
        <errorMessage>The Meeting Day selected must be within 3 +/- days of the start or end of the meeting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Negative_Amount_MERC</fullName>
        <active>true</active>
        <description>Prevent a negative amount from being entered.</description>
        <errorConditionFormula>(ISCHANGED(Daily_Food_Beverage_Amount_MERC__c) &amp;&amp;
Daily_Food_Beverage_Amount_MERC__c &lt; 0 ) ||

(ISCHANGED(Daily_Ground_Trans_Amount_MERC__c ) &amp;&amp;
Daily_Ground_Trans_Amount_MERC__c &lt; 0 ) ||

(ISCHANGED(Daily_Registration_Amount_MERC__c ) &amp;&amp;
Daily_Registration_Amount_MERC__c &lt; 0)</errorConditionFormula>
        <errorMessage>You may not enter a value less than zero.</errorMessage>
    </validationRules>

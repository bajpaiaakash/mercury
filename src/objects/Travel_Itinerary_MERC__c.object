    <fields>
        <fullName>Travel_Proposal_MERC__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Self lookup to ensure Individual Travel Segments can be associated to a specific Travel Proposal : MERC : Created on 16/04/2015 by Oliver Dunford, Mavens Consulting</description>
        <externalId>false</externalId>
        <label>Travel Proposal</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>$Source.RecordTypeId</field>
                <operation>equals</operation>
                <value>Air, Bus, Rail</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Travel_Itinerary_MERC__c</referenceTo>
        <relationshipLabel>Travel Itineraries</relationshipLabel>
        <relationshipName>Travel_Itineraries</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Travel_Proposal_Status_MERC__c</fullName>
        <description>The Status of the associated Travel Proposal, used by the Marketing Cloud to return the correct records : MERC : Created on 04/09/2015 by Oliver Dunford, Mavens Consulting</description>
        <externalId>false</externalId>
        <formula>IF((TEXT(  Travel_Proposal_MERC__r.Status_MERC__c ) = &apos;Ready for Proposal&apos; || TEXT(  Travel_Proposal_MERC__r.Status_MERC__c ) = &apos;Proposed&apos;), &apos;Ready for Proposal/Proposed&apos;, TEXT(Travel_Proposal_MERC__r.Status_MERC__c))</formula>
        <label>Travel Proposal Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>

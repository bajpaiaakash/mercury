        <errorConditionFormula>(
! Confirm_ToV_Reporting_Change_MERC__c &amp;&amp;
ISPICKVAL( Meeting_Participant_MERC__r.Meeting_MERC__r.Participant_ToV_Final_MERC__c , &quot;Yes&quot;) &amp;&amp; (RecordType.DeveloperName = &quot;Hotel_MERC&quot;) &amp;&amp;
(
(ISCHANGED( Sgmnt_Bgin_TS_MERC__c ) || ISCHANGED( Sgmnt_End_TS_MERC__c ) || ISCHANGED(  Meeting_Vendor_MERC__c  )) ||
(ISCHANGED( Status_MERC__c ) &amp;&amp; ISPICKVAL( Status_MERC__c , &quot;Cancelled&quot;))
)
)

||

(
! Confirm_ToV_Reporting_Change_MERC__c &amp;&amp;
ISPICKVAL( Meeting_Participant_MERC__r.Meeting_MERC__r.Participant_ToV_Final_MERC__c , &quot;Yes&quot;) &amp;&amp; (RecordType.DeveloperName = &quot;Car_MERC&quot;) &amp;&amp; ( Individual_Travel_ToV_Applicable_MERC__c  = TRUE) &amp;&amp;
(
(ISCHANGED( Sgmnt_Bgin_TS_MERC__c ) || ISCHANGED( Sgmnt_End_TS_MERC__c ) || ISCHANGED(  Total_Fare_Amount_MERC__c  )) ||
(ISCHANGED( Status_MERC__c ) &amp;&amp; ISPICKVAL( Status_MERC__c , &quot;Cancelled&quot;)) ||
(ISCHANGED( Individual_Travel_ToV_Applicable_MERC__c  ) &amp;&amp; ( Individual_Travel_ToV_Applicable_MERC__c = TRUE))
)
)</errorConditionFormula>
        <errorDisplayField>Confirm_ToV_Reporting_Change_MERC__c</errorDisplayField>
        <errorMessage>ToV has already been reported this meeting participant. Customer Meeting Services needs to confirm this change before it can be made.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MERC_Enforce_Total_Fare_Amount</fullName>
        <active>true</active>
        <description>Ensures  a Total Fare Amount is entered for all Private Transfer Records where ToV should be applicable : MERC : Created on 24/07/2014 by Oliver Dunford, Mavens Consulting</description>
        <errorConditionFormula>Individual_Travel_ToV_Applicable_MERC__c = TRUE &amp;&amp; (ISBLANK( Total_Fare_Amount_MERC__c ) || (Total_Fare_Amount_MERC__c &lt;= 0))</errorConditionFormula>
        <errorDisplayField>Total_Fare_Amount_MERC__c</errorDisplayField>
        <errorMessage>If Individual Travel ToV is applicable for private transfer the Total Fare Amount must be entered and must also be greater than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MERC_Prevent_Private_Transfer_ToV</fullName>
        <active>true</active>
        <description>Prevents the ToV for Individual Transfer checkbox being set if the Meeting Participant is being sponsored for an Independent Meeting.  ToV should be typed into the meeting participant : MERC : Created on 24/07/2014 by Oliver Dunford, Mavens Consulting</description>
        <errorConditionFormula>Meeting_Participant_MERC__r.RecordType.DeveloperName = &quot;Individual_Sponsorship&quot; &amp;&amp; Individual_Travel_ToV_Applicable_MERC__c = TRUE</errorConditionFormula>
        <errorDisplayField>Individual_Travel_ToV_Applicable_MERC__c</errorDisplayField>
        <errorMessage>For Individual Sponsorship all ToV should be totalled and entered onto the meeting participant record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Travel_Records_post_ToV_Final</fullName>
        <active>true</active>
        <description>Prevent  people from adding a new hotel records or travel records that effect ToV once it is final.  Created by John Adams 4/7/2014</description>
        <errorConditionFormula>ISNEW() &amp;&amp; ISPICKVAL( Meeting_Participant_MERC__r.Meeting_MERC__r.Participant_ToV_Final_MERC__c , &quot;Yes&quot;)</errorConditionFormula>
        <errorMessage>ToV for the meeting has already been finalised.  No additional Itinerary items can be created.  Please contact a member of the customer meeting services team.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Travel_Date_Range</fullName>
        <active>true</active>
        <description>Ensure that a Travel/Segment date is within 3 days +/- of the Meeting Start or End Date.  This is to accommodate shoulder dates but also limit the dates that can be chosen.  Created on 02/17/2014 by KLorenti.  Modified 4/21/2014 J Adams</description>
        <errorConditionFormula>(

/* Rail-Flight Segment starts in within 3 days of Meeting start */
 ((RecordType.Name&lt;&gt;&quot;Hotel&quot;) &amp;&amp; Sgmnt_Bgin_TS_MERC__c &lt; ( Meeting_Start_Date_Time_MERC__c - 4))||

/* Rail-Flight Segment ends within 3 days shoulder of Meeting end */
 ((RecordType.Name&lt;&gt;&quot;Hotel&quot;) &amp;&amp; Sgmnt_End_TS_MERC__c &gt; ( Meeting_End_Date_Time_MERC__c + 4)) ||

/* Hotel Segment starts in within 6 days of Meeting start */
 ((RecordType.Name=&quot;Hotel&quot;) &amp;&amp; Sgmnt_Bgin_TS_MERC__c &lt; ( Meeting_Start_Date_Time_MERC__c - 4))||
 ((RecordType.Name=&quot;Hotel&quot;) &amp;&amp; Sgmnt_Bgin_TS_MERC__c &gt; ( Meeting_Start_Date_Time_MERC__c + 3))||

/* Hotel Segment ends sooner than in within 3 days shoulder of Meeting end  */
( (RecordType.Name=&quot;Hotel&quot;) &amp;&amp; (Sgmnt_End_TS_MERC__c &lt;( Meeting_End_Date_Time_MERC__c  - 3))) ||

/* Hotel Segment ends no later than 3 days after Meeting end */
  (RecordType.Name=&quot;Hotel&quot;) &amp;&amp;
    (Sgmnt_End_TS_MERC__c &gt;
      ( Meeting_Start_Date_Time_MERC__c +
        (
           ( Meeting_End_Date_Time_MERC__c - Meeting_Start_Date_Time_MERC__c )
        +4 )
      )
   )
)</errorConditionFormula>
        <errorMessage>The Segment Dates selected must be within 3 +/- days of the start or end of the meeting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Travel_Itinerary_currency_MERC</fullName>
        <active>true</active>
        <description>Validation rule to ensure user sets the currency of the travel itinerary items the same as the meeting.</description>
        <errorConditionFormula>TEXT(CurrencyIsoCode) &lt;&gt;  TEXT( Meeting_Participant_MERC__r.Meeting_MERC__r.CurrencyIsoCode)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>The travel itinerary items must be entered in the same currency as the meeting.</errorMessage>
    </validationRules>
</CustomObject>

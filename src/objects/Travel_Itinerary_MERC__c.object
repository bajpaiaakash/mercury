        <label>Segment End Timestamp</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Status_MERC__c</fullName>
        <description>If the HCP cancels travel or does not use it, the record can be Cancelled.  This then removes any ToV from the associated meeting for that HCP.  Oliver Dunford 11th June 2014.</description>
        <externalId>false</externalId>
        <inlineHelpText>If the HCP cancels attendance, or the ToV attributed to this record should not be reported within Mercury due to a No Show, the record can be updated to Cancelled.  Confirmed records are reportable.</inlineHelpText>
        <label>Status</label>
        <picklist>
            <picklistValues>
                <fullName>Confirmed</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Cancelled</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Draft</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Proposed</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Accepted</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Rejected / New Proposal</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Ticketed</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Travel_Proposal_MERC__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Self lookup to ensure Individual Travel Segments can be associated to a specific Travel Proposal : MERC : Created on 16/04/2015 by Oliver Dunford, Mavens Consulting</description>
        <externalId>false</externalId>
        <label>Travel Proposal</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>$Source.RecordTypeId</field>
                <operation>equals</operation>
                <value>Air, Bus, Rail</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Travel_Itinerary_MERC__c</referenceTo>
        <relationshipLabel>Travel Itineraries</relationshipLabel>
        <relationshipName>Travel_Itineraries</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <recordTypes>
        <fullName>MERC_Travel_Proposal</fullName>
        <active>true</active>
        <description>Used to create Travel Proposals for Meeting Participants</description>
        <label>Travel Proposal</label>
        <picklistValues>
            <picklist>Status_MERC__c</picklist>
            <values>
                <fullName>Accepted</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Cancelled</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Draft</fullName>
                <default>true</default>
            </values>
            <values>
                <fullName>Proposed</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Rejected %2F New Proposal</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Ticketed</fullName>
                <default>false</default>
            </values>
        </picklistValues>
    </recordTypes>

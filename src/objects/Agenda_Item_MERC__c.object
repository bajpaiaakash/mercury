    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>MERC_Agenda_Item_Change</fullName>
        <active>true</active>
        <description>Confirm that the agenda item should be changed : MERC : Created on 12/19/2014 by John Adams, Mavens Consulting</description>
        <errorConditionFormula>TEXT(Meeting_MERC__r.Agenda_Status_MERC__c) = &quot;Final&quot; &amp;&amp;  Cnfrm_Chg_Fnl_Agnd_MERC__c == false</errorConditionFormula>
        <errorMessage>Please confirm you would like to change an item on the Final Agenda.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MERC_Ensure_End_After_Start</fullName>
        <active>true</active>
        <description>Rule to prevent entering an end time for a session before it starts : MERC : Created on 10/03/2013 by John Adams, Mavens Consulting</description>
        <errorConditionFormula>CreatedDate &gt;  LastModifiedDate</errorConditionFormula>
        <errorDisplayField>End_Time_MERC__c</errorDisplayField>
        <errorMessage>The agenda item end time must be after the start time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MERC_Prevent_Change_Once_on_Web_Service</fullName>
        <active>true</active>
        <description>Once a meeting is activated on the web service Agenda Item Name and Agenda Item Description can only be changed by CMS Cork : MERC : Created on 22/07/2014 by Oliver Dunford, Mavens Consulting</description>
        <errorConditionFormula>(ISCHANGED( Name ) || ISCHANGED(  Agenda_Item_Description_MERC__c )) &amp;&amp;  Meeting_MERC__r.Expose_on_Portal_MERC__c = TRUE &amp;&amp;  Confirm_Web_Service_Change_MERC__c = FALSE &amp;&amp; AND(NOT(CONTAINS(LastModifiedBy.Username,&apos;mercuryintegration.veeva@&apos;)))</errorConditionFormula>
        <errorMessage>Once a meeting has been published on the Web Service, only CMS Cork can change the Agenda Item Name and Agenda Item Description. Please contact a member of the CMS Cork team to request a change to this information.</errorMessage>
    </validationRules>

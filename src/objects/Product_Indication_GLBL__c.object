    <validationRules>
        <fullName>Status_Rule</fullName>
        <active>true</active>
        <description>Status of Indication (A – Active, I – Inactive)
Created by John A Adams 11/1/2013</description>
        <errorConditionFormula>NOT(REGEX(Status__c,&quot;[AaIi]&quot;))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status of Indication must be either (A – Active, I – Inactive)</errorMessage>

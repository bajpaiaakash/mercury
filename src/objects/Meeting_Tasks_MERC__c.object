        <externalId>false</externalId>
        <formula>Meeting_Process_MERC__r.Meeting_Participant_MERC__c</formula>
        <label>Meeting Participant</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Meeting_Process_Logic_Key_MERC__c</fullName>
        <description>Used to match a corresponding key in the Meeting Process Logic object to return a match and detail the following automated steps : MERC : Created on 22/06/2015 by Oliver Dunford, Mavens Consulting</description>
        <externalId>false</externalId>
        <formula>TEXT(Meeting_Process_MERC__r.Process_Type_MERC__c) &amp; &quot;;&quot; &amp; TEXT( Task_Status_MERC__c )</formula>
        <label>Meeting Process Logic Key</label>
        <required>false</required>

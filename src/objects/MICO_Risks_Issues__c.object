        <errorConditionFormula>Date_Raised__c &gt;TODAY()</errorConditionFormula>
        <errorDisplayField>Date_Raised__c</errorDisplayField>
        <errorMessage>&quot;Date Raised&quot; cannot be in the future</errorMessage>

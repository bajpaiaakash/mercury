    <fields>
        <fullName>Weighting__c</fullName>
        <externalId>false</externalId>
        <formula>CASE( Impact_CC__c , &quot;Critical&quot;, 3, &quot;High Impact&quot;, 2,&quot;Low Impact&quot;,1, 0)*
CASE(  Probability__c  , &quot;High&quot;, 3, &quot;Medium&quot;, 2,&quot;Low&quot;,1, 3)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>An indicator as to the importance of this risk or issue. Calculated as an impact score multiplied by a probability score.</inlineHelpText>
        <label>Weighting</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>

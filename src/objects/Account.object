<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <fields>
        <fullName>MERC_Allocated_CAP__c</fullName>
        <description>Committed money, but service not yet performed. After meeting owner final fee confirmation, which happens after HCP contract requested by Meeting owner and shortly before contract preparation. Cap is released in case of Participant paid for that meeting. Created 09/02/2016 by Jyoti Gaur</description>
        <externalId>false</externalId>
        <formula>(MERC_Allocated_Fee_for_Current_Year__c + MERC_Allocated_Fee_for_Next_Yr_Cons_Only__c + MERC_Allocated_Fee_for_6_months__c) +
(MERC_Paid_Cap_for_Cons_CN_Used_for_AC__c + MERC_Paid_Cap_for_NonCons_CN_Used_for_AC__c + MERC_Paid_CN_after_meeting_end_date__c) -
(MERC_Paid_Cap_for_Cons_Inv_Used_for_AC__c + MERC_Paid_Cap_for_NonCon_Inv_Used_for_AC__c + MERC_Paid_Invoice_after_meeting_end_date__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Committed money, but service not yet performed. Cap is released in case of  Participant paid for that meeting.</inlineHelpText>
        <label>Allocated CAP</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>

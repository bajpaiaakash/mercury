    <fields>
        <fullName>MERC_Paid_CAP__c</fullName>
        <description>Consolidates Paid CAP from Invoices / Credit Notes for Consulting / Non-Consulting meetings. Created 09/02/2016 by Jyoti Gaur.</description>
        <externalId>false</externalId>
        <formula>(MERC_Paid_Invoices_for_Cons_meeting__c + MERC_Paid_Invoices_for_Non_Cons_meeting__c) - (MERC_Paid_Credit_Note_for_Cons_meeting__c + MERC_Paid_Credit_Note_for_Non_Cons_mtng__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Paid CAP</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>

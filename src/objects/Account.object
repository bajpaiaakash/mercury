<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <fields>
        <fullName>Account_Auto_Number_GLBL__c</fullName>
        <description>Unique Auto Number field used by Web Services to create a secure/obfuscated Account Id. Created by John Lorenti 2014-05-05.</description>
        <displayFormat>A-{000000}</displayFormat>
        <externalId>true</externalId>
        <label>Account_Auto_Number</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </fields>
    <fields>
        <fullName>CV_Upload_File_Name_MERC__c</fullName>
        <description>Set by the portal when the HCP uploads his/her CV</description>
        <externalId>false</externalId>
        <inlineHelpText>Set automatically by the Web Service when a HCP uploads a CV.  This should be set manually by CMS Cork should a HCP follow a paper based process and require their CV verifying.</inlineHelpText>
        <label>CV Upload File Name</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CV_Upload_Step_Complete_MERC__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Used by portal to indicate that the CV Upload sub-step is complete on the HCP Information Package, also used for paper based process and set by CMS Cork. Created on 04/03/2014 by KLorenti, Mavens Consulting</description>
        <externalId>false</externalId>
        <inlineHelpText>Set automatically by the Web Service if a HCP uploads their CV.  Should be set manually by CMS Cork should a HCP be following a paper based process and their CV needs uploading for verification.</inlineHelpText>
        <label>CV Upload Step Complete</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Contract_Signatory_MERC__c</fullName>

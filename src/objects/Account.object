    <fields>
        <fullName>Qualifications_Experience_Complete_MERC__c</fullName>
        <description>Used by portal to indicate that the Qualifications and Experience  overall step is complete on the HCP Information Package. Created on 04/03/2014 by KLorenti, Mavens Consulting</description>
        <externalId>false</externalId>
        <formula>IF(
  (TEXT(HCP_Info_Package_Status_MERC__c) = &quot;Submitted&quot;) ||
  (Qualifications_Step_Complete_MERC__c &amp;&amp;
   Experience_Step_Complete_MERC__c &amp;&amp;
   Research_Publications_Step_Complete_MERC__c &amp;&amp; 
   Teaching_Advising_Step_Complete_MERC__c &amp;&amp;
   Leadership_Committee_Step_Complete_MERC__c &amp;&amp;
     IF(ISPICKVAL(Multiple_Specialties_MERC__c,&quot;Yes&quot;),CV_Upload_Step_Complete_MERC__c,true))
, true, false)</formula>
        <label>Qualifications and Experience Complete</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>

	<fields>
        <fullName>Remaining_CAP_MERC__c</fullName>
        <description>Calculates remaining CAP for the current calendar year.  Annual Country CAP stamped from custom settings and updated each year via Apex batch job. Oliver Dunford 7th Nov 2013.</description>
        <externalId>false</externalId>
        <formula>Annual_CAP_Limit_MERC__c - ( Reserved_CAP_MERC__c +  MERC_Allocated_CAP__c +  MERC_Paid_CAP__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Remaining CAP</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Utilised_CAP_MERC__c</fullName>
        <description>Shows how much CAP has been utilised for the current year for each HCP. Oliver Dunford 7th Nov 2013.</description>
        <externalId>false</externalId>
        <formula>IF (!ISBLANK(TEXT(Country_of_Residence_GLBL__c)), 
(Reserved_CAP_MERC__c +  MERC_Allocated_CAP__c + MERC_Paid_CAP__c )  /  Annual_CAP_Limit_MERC__c,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Utilised CAP</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Percent</type>
    </fields>

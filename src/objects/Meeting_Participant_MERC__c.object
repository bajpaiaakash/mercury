    <fields>
        <fullName>Account_MERC__c</fullName>
        <description>Lookup to Account from Event Participant. Created 9/28/2013 by KLorenti.</description>
        <externalId>false</externalId>
        <label>Account</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>1 OR 2 OR 3</booleanFilter>
            <filterItems>
                <field>Account.RecordType.DeveloperName</field>
                <operation>equals</operation>
                <value>Employee_GLBL</value>
            </filterItems>
            <filterItems>
                <field>Account.RecordType.DeveloperName</field>
                <operation>equals</operation>
                <value>HCP_GLBL</value>
            </filterItems>
            <filterItems>
                <field>Account.RecordType.DeveloperName</field>
                <operation>equals</operation>
                <value>Non_HCP_MERC</value>
            </filterItems>
            <infoMessage>Only HCP or Employee accounts can be added over here.</infoMessage>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Meeting Participants</relationshipLabel>
        <relationshipName>Event_Participants</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Hotel_Name_MERC__c</fullName>
        <description>Field used to display the hotel name of a Meeting Participant in report LINK_Master_report_MERC. Created 11/03/2016 by Edith Sereno, Mavens Consulting.</description>
        <externalId>false</externalId>
        <label>Hotel Name</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Local_Legal_Compliance_Complete_MERC__c</fullName>
        <description>Used to display the status of local / compliance step. Created 07/03/2016 by Edith Sereno, Mavens Consulting</description>
        <externalId>false</externalId>
        <inlineHelpText>Is compliance necessary for this Meeting Participant? If yes, have they provided the required documentation?</inlineHelpText>
        <label>Local Legal/Compliance Complete</label>
        <picklist>
            <picklistValues>
                <fullName>Yes</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>No</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Not Applicable</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Meeting_Group_MERC__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Lookup Relationship from Meeting Participant to Meeting Group. Created 02/03/2016 by Edith Sereno, Mavens Consulting.</description>
        <externalId>false</externalId>
        <label>Meeting Group</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>Meeting_Group_MERC__c.Meeting_MERC__c</field>
                <operation>equals</operation>
                <valueField>$Source.Meeting_MERC__c</valueField>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Meeting_Group_MERC__c</referenceTo>
        <relationshipLabel>Meeting Participants</relationshipLabel>
        <relationshipName>Meeting_Participants</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Registration_Status_MERC__c</fullName>
        <description>Created to track the status of Meeting Participants registration (web and paper processes). Created 03/03/2016 by Mavens Consulting</description>
        <externalId>false</externalId>
        <inlineHelpText>This field shows if a Meeting Participant registration has been validated.</inlineHelpText>
        <label>Registration Status</label>
        <picklist>
            <controllingField>Status_MERC__c</controllingField>
            <picklistValues>
                <fullName>Pending Review</fullName>
                <controllingFieldValues>Accepted</controllingFieldValues>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Complete</fullName>
                <controllingFieldValues>Accepted</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Incomplete</fullName>
                <controllingFieldValues>Accepted</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>

	<fields>
        <fullName>Active_on_Web_Service_MERC__c</fullName>
        <description>Indicates whether or not the Meeting Participant should be available on the Web Service.  Created 05/15/2014 by KLorenti, Mavens Consulting</description>
        <externalId>false</externalId>
        <formula>CASE(Status_MERC__c,
&apos;Invited&apos;, 1,
&apos;Accepted&apos;, 1,
&apos;Declined&apos;, 1, 
0) == 1</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Active on Web Service</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>

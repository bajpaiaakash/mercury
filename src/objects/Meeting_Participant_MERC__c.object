        <fullName>Cancelled_Meeting_with_Part_Pymnt_MER</fullName>
        <active>false</active>
        <description>Partial Payments should not be entered unless meeting is Cancelled.</description>
        <errorConditionFormula>!ISPICKVAL(Meeting_MERC__r.Meeting_Status_MERC__c, &quot;Cancelled&quot;)&amp;&amp;
ISCHANGED(Partial_Payment_Amount_MERC__c) &amp;&amp; !ISBLANK(Partial_Payment_Amount_MERC__c)</errorConditionFormula>
        <errorDisplayField>Partial_Payment_Amount_MERC__c</errorDisplayField>
        <errorMessage>The Partial Payment Amount may only be entered if the Meeting is in a Cancelled status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_if_any_ToV_has_been_reported</fullName>
        <active>true</active>
        <description>Checks to see if any Meeting Participants have had ToV reported. Created 01/08/2014 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>ISPICKVAL( Meeting_MERC__r.Participant_ToV_Final_MERC__c,&quot;Yes&quot;) &amp;&amp;
(
ISCHANGED ( Total_Registration_ToV_MERC__c  ) ||
ISCHANGED ( Total_Ground_Transporation_ToV_MERC__c ) ||
ISCHANGED ( Total_Food_Beverage_ToV_MERC__c ) ||
ISCHANGED ( CoPay_Hotel_MERC__c ) ||
ISCHANGED ( CoPay_Ground_Transport_MERC__c ) ||
ISCHANGED ( CoPay_Registration_MERC__c ) ||
ISCHANGED ( CoPay_Food_Beverage_MERC__c ) ||
ISCHANGED ( CoPay_Flight_Rail_MERC__c )
) 
&amp;&amp;  !Confirm_ToV_Reporting_Change_MERC__c</errorConditionFormula>
        <errorDisplayField>Confirm_ToV_Reporting_Change_MERC__c</errorDisplayField>
        <errorMessage>ToV has already been reported for participants of this Meeting. Customer Meeting Services needs to confirm this change before it can be made.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirm_Final_Fee</fullName>
        <active>true</active>
        <description>Unable to Confirm Final Fee if Exception Fee Approval is Required or Rejected.</description>
        <errorConditionFormula>(ISPICKVAL(Higher_Fee_Approval__c,&apos;Required&apos;) ||  ISPICKVAL(Higher_Fee_Approval__c,&apos;Rejected&apos;)) &amp;&amp; ISPICKVAL(Confirm_Final_Fee_MERC__c,&apos;Confirmed&apos;)</errorConditionFormula>
        <errorDisplayField>Confirm_Final_Fee_MERC__c</errorDisplayField>
        <errorMessage>You cannot confirm the final fee if the Exception Approval is Required or Rejected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirm_Partial_Payment_MERC</fullName>
        <active>true</active>
        <description>Confirm Final Fee should be updated with the Partial Payment Amount</description>
        <errorConditionFormula>!Confirm_Partial_Payment_Amount_MERC__c &amp;&amp;
ISCHANGED(Partial_Payment_Amount_MERC__c) &amp;&amp;
Partial_Payment_Amount_MERC__c &gt; 0.00 &amp;&amp;
TEXT(Confirm_Final_Fee_MERC__c) = &quot;Confirmed&quot; &amp;&amp; TEXT(Services_Rendered_MERC__c) = &quot;Yes&quot;</errorConditionFormula>
        <errorDisplayField>Confirm_Partial_Payment_Amount_MERC__c</errorDisplayField>
        <errorMessage>Please confirm the Final Fee should be updated with the Partial Payment Amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Blank_If_Not_Reported_MERC</fullName>
        <active>true</active>
        <description>Users cannot enter a 0.00 in a CoPay if ToV has not been reported.</description>
        <errorConditionFormula>( 
(ISCHANGED(Total_Hotel_ToV_MERC__c) &amp;&amp; Total_Hotel_ToV_MERC__c = 0.00) || 
(ISCHANGED(Total_Ground_Transporation_ToV_MERC__c) &amp;&amp; Total_Ground_Transporation_ToV_MERC__c = 0.00) || 
(ISCHANGED(Total_Food_Beverage_ToV_MERC__c) &amp;&amp; Total_Food_Beverage_ToV_MERC__c = 0.00) || 
(ISCHANGED(Total_Registration_ToV_MERC__c) &amp;&amp; Total_Registration_ToV_MERC__c = 0.00) || 
(ISCHANGED(CoPay_Flight_Rail_MERC__c) &amp;&amp; CoPay_Flight_Rail_MERC__c = 0.00) || 
(ISCHANGED(CoPay_Food_Beverage_MERC__c) &amp;&amp; CoPay_Food_Beverage_MERC__c = 0.00) || 
(ISCHANGED(CoPay_Ground_Transport_MERC__c) &amp;&amp; CoPay_Ground_Transport_MERC__c = 0.00) || 
(ISCHANGED(CoPay_Hotel_MERC__c) &amp;&amp; CoPay_Hotel_MERC__c = 0.00) || 
(ISCHANGED(CoPay_Registration_MERC__c) &amp;&amp; CoPay_Registration_MERC__c = 0.00) 
) 

&amp;&amp; 

(ISPICKVAL(Meeting_MERC__r.Participant_ToV_Final_MERC__c,&quot;No&quot;) ||
ISPICKVAL(Meeting_MERC__r.Participant_ToV_Final_MERC__c,&quot;&quot;))</errorConditionFormula>
        <errorMessage>You may not enter 0.00 unless ToV has already been reported for this Meeting</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Zero_If_ToV_Reported_MERC</fullName>
        <active>true</active>
        <description>Users cannot enter a blank if ToV has been reported.</description>
        <errorConditionFormula>(
(ISCHANGED(Total_Hotel_ToV_MERC__c) &amp;&amp;  ISBLANK(Total_Hotel_ToV_MERC__c)) || 
 (ISCHANGED(Total_Ground_Transporation_ToV_MERC__c) &amp;&amp; ISBLANK(Total_Ground_Transporation_ToV_MERC__c)) || 
 (ISCHANGED(Total_Food_Beverage_ToV_MERC__c) &amp;&amp; ISBLANK(Total_Food_Beverage_ToV_MERC__c)) || 
 (ISCHANGED(Total_Registration_ToV_MERC__c) &amp;&amp; ISBLANK(Total_Registration_ToV_MERC__c)) || 
 (ISCHANGED(CoPay_Flight_Rail_MERC__c) &amp;&amp; ISBLANK(CoPay_Flight_Rail_MERC__c)) || 
 (ISCHANGED(CoPay_Food_Beverage_MERC__c) &amp;&amp; ISBLANK(CoPay_Food_Beverage_MERC__c)) || 
 (ISCHANGED(CoPay_Ground_Transport_MERC__c) &amp;&amp; ISBLANK(CoPay_Ground_Transport_MERC__c)) ||
 (ISCHANGED(CoPay_Hotel_MERC__c) &amp;&amp; ISBLANK(CoPay_Hotel_MERC__c)) ||
 (ISCHANGED(CoPay_Registration_MERC__c) &amp;&amp; ISBLANK(CoPay_Registration_MERC__c))
)

&amp;&amp;  

 (ISPICKVAL(Meeting_MERC__r.Participant_ToV_Final_MERC__c,&quot;Yes&quot;))</errorConditionFormula>
        <errorMessage>You may not enter a blank value if ToV has already been reported for this Meeting</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Individual_Sponsorship_Only_MERC</fullName>
        <active>false</active>
        <description>If Meeting is Individual Sponsorship, only allow participants of type Individual Sponsorship</description>
        <errorConditionFormula>ISPICKVAL(Meeting_MERC__r.Sponsorship_Category_MERC__c,
&apos;Individual Sponsorship&apos;) &amp;&amp;   RecordType.Name  &lt;&gt;  &apos;Individual Sponsorship&apos;</errorConditionFormula>
        <errorDisplayField>Account_MERC__c</errorDisplayField>
        <errorMessage>You must enter Participant Type &quot;Individual Sponsorship&quot; for meetings with Meetings of Sponsorship Category &quot;Individual Sponsorship&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partial_Payment_Allowed_MERC</fullName>
        <active>true</active>
        <description>The Partial Payment Amount should only be entered if the Final Fee has been entered and the Participant ToV Final is False.</description>
        <errorConditionFormula>ISCHANGED(Partial_Payment_Amount_MERC__c) &amp;&amp;
Partial_Payment_Amount_MERC__c &gt; 0 &amp;&amp;
(TEXT(Services_Rendered_MERC__c) &lt;&gt; &quot;Yes&quot; ||
Text(Confirm_Final_Fee_MERC__c) &lt;&gt; &quot;Confirmed&quot;)</errorConditionFormula>
        <errorDisplayField>Partial_Payment_Amount_MERC__c</errorDisplayField>
        <errorMessage>You may not enter a Partial Payment Amount if services have not been rendered or if the Final Fee has not been confirmed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Change_Status_if_ToV_Final_MERC</fullName>
        <active>true</active>
        <description>If the Participant ToV Final is checked you should not be able to change the Participant&apos;s attendance status. Created 01/14/2014 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>ISCHANGED(Status_MERC__c) &amp;&amp; 
ISPICKVAL( Meeting_MERC__r.Participant_ToV_Final_MERC__c ,&quot;Yes&quot;)</errorConditionFormula>
        <errorDisplayField>Status_MERC__c</errorDisplayField>
        <errorMessage>You cannot change the Participant&apos;s status if the Participant ToV is final.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Negative_Amount_in_CoPay_MERC</fullName>
        <active>true</active>
        <description>Prevent users from entering negative numbers in CoPay fields</description>
        <errorConditionFormula>(ISCHANGED(CoPay_Flight_Rail_MERC__c) &amp;&amp;
 CoPay_Flight_Rail_MERC__c &lt;0) ||

(ISCHANGED(CoPay_Food_Beverage_MERC__c) &amp;&amp;
 CoPay_Food_Beverage_MERC__c &lt;0) ||

(ISCHANGED(CoPay_Ground_Transport_MERC__c) &amp;&amp;
 CoPay_Ground_Transport_MERC__c &lt;0) ||

(ISCHANGED(CoPay_Hotel_MERC__c) &amp;&amp;
CoPay_Hotel_MERC__c &lt;0) ||

(ISCHANGED(CoPay_Registration_MERC__c) &amp;&amp;
CoPay_Registration_MERC__c &lt;0)</errorConditionFormula>
        <errorMessage>You may not enter a CoPay value less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Proposed_Final_Fee_MERC</fullName>
        <active>true</active>
        <description>The Proposed Final Fee cannot be entered unless a Minimum and Maximum Rate are presented.  Created by John A Adams 2/26/2014</description>
        <errorConditionFormula>ISCHANGED(Proposed_Final_Fee_MERC__c) &amp;&amp; Proposed_Final_Fee_MERC__c &lt;&gt; 0 &amp;&amp;
 (ISBLANK(Minimum_Rate_MERC__c) ||
 ISBLANK(Maximum_Rate_MERC__c))</errorConditionFormula>
        <errorDisplayField>Proposed_Final_Fee_MERC__c</errorDisplayField>
        <errorMessage>You may not enter a Proposed Final Fee if the Minimum and Maximum Hourly Rate values are not present. Tiering must be complete and Country of Residence indicated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Services_Rendered_MERC</fullName>
        <active>true</active>
        <description>Only HCP Care or System Administrator can change Services Rendered value if it is already set to &quot;Yes&quot;.  Created 02/03/2014 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>$Profile.Name &lt;&gt; &quot;Mercury HCP Care&quot; &amp;&amp; 
$Profile.Name &lt;&gt; &quot;System Administrator&quot; &amp;&amp;
ISCHANGED(Services_Rendered_MERC__c) &amp;&amp;
ISPICKVAL(PRIORVALUE(Services_Rendered_MERC__c),&quot;Yes&quot;)</errorConditionFormula>
        <errorDisplayField>Services_Rendered_MERC__c</errorDisplayField>
        <errorMessage>You may not change this value after services has been rendered. Please contact Customer Meeting Services.</errorMessage>
    </validationRules>

<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <enableEnhancedLookup>true</enableEnhancedLookup>
    <validationRules>
        <fullName>MERC_Services_Rendered</fullName>
        <active>true</active>
        <description>Only HCP Care or System Administrator can change Services Rendered value if it is already set to &quot;Yes&quot; : MERC : Created on 02/03/2014 by Katy Lorenti, Mavens Consulting</description>
        <errorConditionFormula>$Profile.Name &lt;&gt; &quot;System Administrator&quot; &amp;&amp; 
($User.Power_User_MERC__c = False ||   
LOWER($User.Case_Application_GLBL__c) != &quot;mercury&quot;)  &amp;&amp; 
ISCHANGED(Services_Rendered_MERC__c) &amp;&amp;
ISPICKVAL(PRIORVALUE(Services_Rendered_MERC__c),&quot;Yes&quot;)</errorConditionFormula>
        <errorDisplayField>Services_Rendered_MERC__c</errorDisplayField>
        <errorMessage>You may not change this value after services has been rendered. Please contact Customer Meeting Services.</errorMessage>
    </validationRules>
</CustomObject>

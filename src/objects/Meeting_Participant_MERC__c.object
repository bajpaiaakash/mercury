        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Local_Legal_Compliance_Complete_MERC__c</fullName>
        <description>Used to display the status of local / compliance step. Created 07/03/2016 by Edith Sereno, Mavens Consulting</description>
        <externalId>false</externalId>
        <inlineHelpText>Is compliance necessary for this Meeting Participant? If yes, have they provided the required documentation?</inlineHelpText>
        <label>Local Legal/Compliance Complete</label>
        <picklist>
            <picklistValues>
                <fullName>Yes</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>No</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Not Applicable</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Registration_Status_MERC__c</fullName>
        <description>Created to track the status of Meeting Participants registration (web and paper processes). Created 03/03/2016 by Mavens Consulting</description>
        <externalId>false</externalId>
        <inlineHelpText>This field shows if a Meeting Participant registration has been validated.</inlineHelpText>
        <label>Registration Status</label>
        <picklist>
            <controllingField>Status_MERC__c</controllingField>
            <picklistValues>
                <fullName>Pending Review</fullName>
                <controllingFieldValues>Accepted</controllingFieldValues>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Complete</fullName>
                <controllingFieldValues>Accepted</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Incomplete</fullName>
                <controllingFieldValues>Accepted</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>

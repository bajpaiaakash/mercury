&quot;Marketing&quot;, &quot;M&quot;,
&quot;Medical&quot;,&quot;R&quot;,
&quot;Corporate Affairs&quot;,&quot;C&quot;,
&quot;Other&quot;,&quot;O&quot;,
&quot;&quot;) 
&amp; 
CASE(  Meeting_Scope_MERC__c   , 
&quot;Local&quot;, &quot;L&quot;,
&quot;National&quot;, &quot;N&quot;,
&quot;Cluster&quot;,&quot;C&quot;,
&quot;International&quot;,&quot;I&quot;,

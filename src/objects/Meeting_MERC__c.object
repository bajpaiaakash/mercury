    <validationRules>
        <fullName>Active_on_Web_Services_MERC</fullName>
        <active>false</active>
        <description>When Meeting has already been Activated on Web Services, prevent the deactivation. Created 04/02/2014</description>
        <errorConditionFormula>ISCHANGED(Expose_on_Portal_MERC__c) = true &amp;&amp;
PRIORVALUE(Expose_on_Portal_MERC__c) = true</errorConditionFormula>
        <errorDisplayField>Expose_on_Portal_MERC__c</errorDisplayField>
        <errorMessage>Once the Meeting has been activated on Web Service, it cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Compliance_Risk_before_Complete_MERC</fullName>
        <active>true</active>
        <description>The Compliance Risk field must be set to yes before the Meeting can be Completed.  Created 12/18/2013 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>RecordType.Name &lt;&gt; &apos;Consulting Project&apos; &amp;&amp;
RecordType.Name &lt;&gt; &apos;Independent Meeting&apos; &amp;&amp;
RecordType.Name &lt;&gt; &apos;Meeting Program&apos; &amp;&amp;
ISBLANK(Text(Compliant_MERC__c)) &amp;&amp; 
ISCHANGED(Meeting_Status_MERC__c) &amp;&amp;
ISPICKVAL(Meeting_Status_MERC__c,&apos;Complete&apos;)</errorConditionFormula>
        <errorDisplayField>Compliant_MERC__c</errorDisplayField>
        <errorMessage>The Compliance Risk must be selected in order to Complete the Meeting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirm_Budget_is_Final_MERC</fullName>
        <active>true</active>
        <description>Budget must be final before Meeting can be completed. Created 11/19/2013 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>(RecordType.Name &lt;&gt; &apos;Independent Meeting&apos; &amp;&amp;
RecordType.Name &lt;&gt; &apos;Consulting Project&apos; &amp;&amp; 
RecordType.Name &lt;&gt; &apos;Meeting Program&apos;) &amp;&amp; 
!ISPICKVAL(Budget_is_Final_MERC__c,&apos;Yes&apos;) &amp;&amp; 
(ISPICKVAL(Meeting_Status_MERC__c,&apos;Complete&apos;) || 
ISPICKVAL(Meeting_Status_MERC__c,&apos;Complete - Closed&apos;))</errorConditionFormula>
        <errorDisplayField>Budget_is_Final_MERC__c</errorDisplayField>
        <errorMessage>The Budget must be Final before the Meeting can be Completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirm_MCP_Evaluation_MERC</fullName>
        <active>true</active>
        <description>The MPC has been evaluated before the Meeting can be Completed.</description>
        <errorConditionFormula>RecordType.Name &lt;&gt; &apos;Independent Meeting&apos;
&amp;&amp; RecordType.Name &lt;&gt; &apos;Consulting Project&apos;
&amp;&amp; RecordType.Name &lt;&gt; &apos;Meeting Program&apos;
&amp;&amp; ISPICKVAL( MCP_work_MERC__c , &quot;&quot;)
&amp;&amp; ISPICKVAL(Meeting_Status_MERC__c, &apos;Complete - Closed&apos;)</errorConditionFormula>
        <errorDisplayField>MCP_work_MERC__c</errorDisplayField>
        <errorMessage>The Meeting Planning Companies performance must be evaluated before completing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirm_Meeting_Cancellation_MERC</fullName>
        <active>true</active>
        <description>Confirm that the Meeting should be cancelled. Created 11/19/2013 by KLorenti, Mavens Consulting.</description>
        <errorConditionFormula>!ISPICKVAL(Event_Occurred_MERC__c,&quot;Yes&quot;) &amp;&amp;
ISCHANGED( Meeting_Status_MERC__c) &amp;&amp; 
ISPICKVAL( Meeting_Status_MERC__c ,&apos;Complete - Cancelled&apos;) &amp;&amp;  
!Confirm_Meeting_Cancellation_MERC__c</errorConditionFormula>
        <errorDisplayField>Confirm_Meeting_Cancellation_MERC__c</errorDisplayField>
        <errorMessage>Please confirm you would like this Meeting to be Cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirm_Meeting_Date_Change_MERC</fullName>
        <active>true</active>
        <description>Enforces confirmation of Meeting Date changes. Created 10/24/2013 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>!ISNEW ()&amp;&amp;
(
ISCHANGED(Meeting_Time_Zone_MERC__c) ||
ISCHANGED(Start_Time_of_Meeting_MERC__c) ||
ISCHANGED(End_Time_of_Meeting_MERC__c) 
) 
&amp;&amp;  !Confirm_Meeting_Date_Change_MERC__c</errorConditionFormula>
        <errorDisplayField>Confirm_Meeting_Date_Change_MERC__c</errorDisplayField>
        <errorMessage>Please agree to/confirm the Meeting Date/Time Change.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Occurred_before_Complete_MERC</fullName>
        <active>true</active>
        <description>The Event Occurred field must be set to yes before the Meeting can be Completed.  Created 11/26/2013 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>RecordType.Name &lt;&gt; &apos;Consulting Project&apos; &amp;&amp;
RecordType.Name &lt;&gt; &apos;Meeting Program&apos; &amp;&amp;
NOT ISPICKVAL(Event_Occurred_MERC__c,&apos;Yes&apos;)&amp;&amp; ISCHANGED(Meeting_Status_MERC__c) &amp;&amp;
ISPICKVAL(Meeting_Status_MERC__c,&apos;Complete - Closed&apos;)</errorConditionFormula>
        <errorDisplayField>Event_Occurred_MERC__c</errorDisplayField>
        <errorMessage>The Meeting must have occurred in order to Complete the Meeting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Jumping_from_Forecasted_to_Active</fullName>
        <active>true</active>
        <description>Status may not be changed from Forecasted to Active. Must go through Registered first.</description>
        <errorConditionFormula>ISCHANGED(Meeting_Status_MERC__c) &amp;&amp;
ISPICKVAL(Meeting_Status_MERC__c,&apos;Active&apos;) &amp;&amp;
ISPICKVAL(PRIORVALUE(Meeting_Status_MERC__c),&apos;Forecasted&apos;)</errorConditionFormula>
        <errorDisplayField>Meeting_Status_MERC__c</errorDisplayField>
        <errorMessage>You may not change the status from Forecasted to Active. Please change status to Registered first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Participant_ToV_Final_only_if_Meet_over</fullName>
        <active>true</active>
        <description>Validate that the meeting end date has passed before allowing the  &apos;Participant ToV Final&apos; field from being checked</description>
        <errorConditionFormula>ISCHANGED(Participant_ToV_Final_MERC__c) &amp;&amp;
(TODAY() &lt;  DATEVALUE(End_Time_of_Meeting_MERC__c ))</errorConditionFormula>
        <errorDisplayField>Participant_ToV_Final_MERC__c</errorDisplayField>
        <errorMessage>The &apos;Participant ToV Final&apos; field cannot be checked until after the meeting is over</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Cancel_Meeting_if_Occurred_MERC</fullName>
        <active>true</active>
        <description>If the Meeting has already occurred you may not cancel the meeting. Created 01/10/2014 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>ISPICKVAL(Event_Occurred_MERC__c,&apos;Yes&apos;)&amp;&amp; ISCHANGED(Meeting_Status_MERC__c) &amp;&amp;
ISPICKVAL(Meeting_Status_MERC__c,&apos;Complete - Cancelled&apos;)</errorConditionFormula>
        <errorDisplayField>Event_Occurred_MERC__c</errorDisplayField>
        <errorMessage>The Meeting cannot be cancelled if it has already occurred.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Participant_ToV_Final_Chg_MERC</fullName>
        <active>true</active>
        <description>Once Participant ToV Final is Yes, it cannot be changed. Created 02/04/2014 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>ISCHANGED(Participant_ToV_Final_MERC__c) &amp;&amp;
ISPICKVAL(PRIORVALUE(Participant_ToV_Final_MERC__c),&quot;Yes&quot;)</errorConditionFormula>
        <errorDisplayField>Participant_ToV_Final_MERC__c</errorDisplayField>
        <errorMessage>You may not change this value once it has been set to Yes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Active_Content_Approval_MERC</fullName>
        <active>true</active>
        <description>Check the required fields based on the Status selected. Created 12/4/2013 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>IF(RecordType.Name = &apos;Meeting Program&apos; ||
   RecordType.Name = &apos;Independent Meeting&apos; ||
   RecordType.Name = &apos;Consulting Project&apos;
,false, 

 IF(ISCHANGED(Meeting_Status_MERC__c) &amp;&amp;
   ISPICKVAL(PRIORVALUE(Meeting_Status_MERC__c),&apos;Registered&apos;) &amp;&amp;
   TEXT(Meeting_Status_MERC__c) = &apos;Active&apos; &amp;&amp;
   ISBLANK(TEXT(Content_Approval_Needed_MERC__c ))

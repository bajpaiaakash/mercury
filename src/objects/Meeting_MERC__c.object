    <validationRules>
        <fullName>Validate_Status_Fields_MERC</fullName>
        <active>false</active>
        <description>Check the required fields based on the Status selected. Created 12/4/2013 by KLorenti, Mavens Consulting (work in progress)</description>
        <errorConditionFormula>IF ( RecordType.Name  &lt;&gt; &apos;Independent Sponsorship&apos;, 
IF(ISCHANGED(Meeting_Status_MERC__c) &amp;&amp; 
RecordType.DeveloperName &lt;&gt; &apos;Independent Sponsorship&apos; ,
	IF(
	  CASE (TEXT(Meeting_Status_MERC__c),
		&apos;Registered&apos;,
			IF(
				(
				ISBLANK(TEXT( Interaction_Type_MERC__c )) ||
				ISBLANK (Number_of_Expected_Deletgates_MERC__c) ||
				ISBLANK (Number_of_Expected_Employees_MERC__c) ||
				ISBLANK (Number_of_Expected_Speakers_MERC__c) ||
				ISBLANK (TEXT(Target_Audience_List_MERC__c )) ||
				ISBLANK ( Preferred_Venue_Addr_1_MERC__c)
				),
			1,
			0),

		&apos;Submitted for Contracting&apos;,
			IF(
				(
				ISBLANK ( Meeting_Objectives_MERC__c ) ||
				ISBLANK ( Content_Creator_MERC__c ) ||
				ISBLANK ( TEXT(Content_Approval_Needed_MERC__c ))
				),
			1,
			0),

		&apos;Ready for Sourcing&apos;,
			IF(
				(
				ISBLANK( Number_of_Expected_Non_HCP_MERC__c ) 
				),
			1,
			0),

		0
		) = 1, true, false),

	false
),
false
)</errorConditionFormula>
        <errorMessage>Please verify required fields below are populated to register the meeting.</errorMessage>
    </validationRules>

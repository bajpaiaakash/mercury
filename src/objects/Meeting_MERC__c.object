    <validationRules>
        <fullName>Confirm_Budget_is_Final_MERC</fullName>
        <active>true</active>
        <description>Budget must be final before Meeting can be completed. Created 11/19/2013 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>RecordType.Name &lt;&gt; &apos;Independent Sponsorship&apos;
&amp;&amp; !ISPICKVAL(Budget_is_Final_MERC__c,&apos;Yes&apos;)
&amp;&amp;  ISPICKVAL(Meeting_Status_MERC__c,&apos;Completed&apos;)</errorConditionFormula>
        <errorDisplayField>Budget_is_Final_MERC__c</errorDisplayField>
        <errorMessage>The Budget must be Final before the Meeting can be Completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirm_Meeting_Cancellation_MERC</fullName>
        <active>true</active>
        <description>Confirm that the Meeting should be cancelled. Created 11/19/2013 by KLorenti, Mavens Consulting.</description>
        <errorConditionFormula>ISCHANGED( Meeting_Status_MERC__c) &amp;&amp; 
ISPICKVAL( Meeting_Status_MERC__c ,&apos;Cancelled&apos;) &amp;&amp;  
!Confirm_Meeting_Cancellation_MERC__c</errorConditionFormula>
        <errorDisplayField>Confirm_Meeting_Cancellation_MERC__c</errorDisplayField>
        <errorMessage>Please confirm you would like this Meeting to be Cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirm_Meeting_Date_Change_MERC</fullName>
        <active>true</active>
        <description>Enforces confirmation of Meeting Date changes. Created 10/24/2013 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>(
ISCHANGED(Date_of_Event_MERC__c) || ISCHANGED(End_Date_of_Event_MERC__c) ||
ISCHANGED( Start_Time_of_Meeting_MERC__c ) ||
ISCHANGED( End_Time_of_Meeting_MERC__c )
) 
&amp;&amp;  !Confirm_Meeting_Date_Change_MERC__c</errorConditionFormula>
        <errorDisplayField>Confirm_Meeting_Date_Change_MERC__c</errorDisplayField>
        <errorMessage>Please agree to/confirm the Meeting Date Change.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Occurred_before_Complete_MERC</fullName>
        <active>true</active>
        <description>The Event Occurred field must be set to yes before the Meeting can be Completed.  Created 11/26/2013 by KLorenti, Mavens Consulting</description>
        <errorConditionFormula>NOT ISPICKVAL(Event_Occurred_MERC__c,&apos;Yes&apos;)&amp;&amp; ISCHANGED(Meeting_Status_MERC__c) &amp;&amp;
ISPICKVAL(Meeting_Status_MERC__c,&apos;Completed&apos;)</errorConditionFormula>
        <errorDisplayField>Event_Occurred_MERC__c</errorDisplayField>
        <errorMessage>The Meeting must have occurred in order to Complete the Meeting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Registered_Status_Fields_MERC</fullName>
        <active>true</active>
        <description>Check the required fields based on the Status selected. Created 12/4/2013 by KLorenti, Mavens Consulting (work in progress)</description>
        <errorConditionFormula>IF(RecordType.Name  &lt;&gt; &apos;Independent Sponsorship&apos; &amp;&amp;
      ISCHANGED(Meeting_Status_MERC__c) &amp;&amp;
		TEXT(Meeting_Status_MERC__c) = &apos;Registered&apos; &amp;&amp;
		(ISBLANK(TEXT( Interaction_Type_MERC__c )) ||
		ISBLANK (Number_of_Expected_Deletgates_MERC__c) ||
 		ISBLANK (Number_of_Expected_Employees_MERC__c) ||
 		ISBLANK (Number_of_Expected_Speakers_MERC__c) ||
 		ISBLANK (TEXT(Target_Audience_List_MERC__c )) ||
 		ISBLANK ( Venue_Preferred_Location_MERC__c))
	,true, false)</errorConditionFormula>
        <errorMessage>Please ensure all fields in the &quot;Required to Register Meeting&quot; section are populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Sourcing_Status_Fields_MERC</fullName>
        <active>true</active>
        <description>Check the required fields based on the Status selected. Created 12/4/2013 by KLorenti, Mavens Consulting (work in progress)</description>
        <errorConditionFormula>IF(RecordType.Name  &lt;&gt; &apos;Independent Sponsorship&apos; &amp;&amp;
      ISCHANGED(Meeting_Status_MERC__c) &amp;&amp;
		TEXT(Meeting_Status_MERC__c) = &apos;Ready for Sourcing&apos; &amp;&amp;
		(ISBLANK(Number_of_Expected_Non_HCP_MERC__c)) 
,true, false)</errorConditionFormula>
        <errorMessage>Please ensure all fields in the &quot;Required for Sourcing&quot; section are populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Status_Fields_MERC</fullName>
        <active>false</active>
        <description>Check the required fields based on the Status selected. Created 12/4/2013 by KLorenti, Mavens Consulting (work in progress)</description>
        <errorConditionFormula>IF ( RecordType.Name  &lt;&gt; &apos;Independent Sponsorship&apos;, 
IF(ISCHANGED(Meeting_Status_MERC__c) &amp;&amp; 
RecordType.DeveloperName &lt;&gt; &apos;Independent Sponsorship&apos; ,
	IF(
	  CASE (TEXT(Meeting_Status_MERC__c),
		&apos;Registered&apos;,
			IF(
				(
				ISBLANK(TEXT( Interaction_Type_MERC__c )) ||
				ISBLANK (Number_of_Expected_Deletgates_MERC__c) ||
				ISBLANK (Number_of_Expected_Employees_MERC__c) ||
				ISBLANK (Number_of_Expected_Speakers_MERC__c) ||
				ISBLANK (TEXT(Target_Audience_List_MERC__c )) ||
				ISBLANK ( Venue_Preferred_Location_MERC__c)
				),
			1,
			0),

		&apos;Submitted for Contracting&apos;,
			IF(
				(
				ISBLANK ( Meeting_Objectives_MERC__c ) ||
				ISBLANK ( Content_Creator_MERC__c ) ||
				ISBLANK ( TEXT(Content_Approval_Needed_MERC__c ))
				),
			1,
			0),

		&apos;Ready for Sourcing&apos;,
			IF(
				(
				ISBLANK( Number_of_Expected_Non_HCP_MERC__c ) 
				),
			1,
			0),

		0
		) = 1, true, false),

	false
),
false
)</errorConditionFormula>
        <errorMessage>Please verify required fields below are populated to register the meeting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Submitted_Status_Fields_MERC</fullName>
        <active>true</active>
        <description>Check the required fields based on the Status selected. Created 12/4/2013 by KLorenti, Mavens Consulting (work in progress)</description>
        <errorConditionFormula>IF(RecordType.Name  &lt;&gt; &apos;Independent Sponsorship&apos; &amp;&amp;
      ISCHANGED(Meeting_Status_MERC__c) &amp;&amp;
		TEXT(Meeting_Status_MERC__c) = &apos;Submitted for Contracting&apos; &amp;&amp;
		(ISBLANK (Meeting_Objectives_MERC__c ) ||
		ISBLANK (Content_Creator_MERC__c ) ||
		ISBLANK (TEXT(Content_Approval_Needed_MERC__c )))
,true, false)</errorConditionFormula>
        <errorMessage>Please ensure all fields in the &quot;Required to Submit for Contracting&quot; section are populated.</errorMessage>
    </validationRules>
